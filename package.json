{
  "name": "metapak-nfroidure",
  "description": "A metapak module for my open source projects.",
  "version": "6.1.1",
  "metapak": {
    "configs": [
      "readme",
      "jest",
      "eslint",
      "travis",
      "codeclimate"
    ],
    "data": {
      "testsFiles": "'src/**/*.mocha.js'",
      "files": "'src/**/*.js'"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfroidure/metapak-nfroidure.git"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cover": "npm run jest -- --coverage",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "jest": "NODE_ENV=test jest",
    "lint": "eslint 'src/**/*.js'",
    "metapak": "metapak",
    "prettier": "prettier --write 'src/**/*.js'",
    "preversion": "npm t && npm run lint && npm run metapak -s",
    "test": "npm run jest",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "author": "Nicolas Froidure",
  "license": "MIT",
  "peerDependencies": {
    "metapak": "^1.0.0"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/metapak-nfroidure/issues"
  },
  "homepage": "https://github.com/nfroidure/metapak-nfroidure#readme",
  "devDependencies": {
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "metapak": "^1.0.3",
    "prettier": "^1.14.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
